{
  "name": "baristagram",
  "version": "1.0.0",
  "description": "Cloning Instagram, For React/Redux Study",
  "main": "server.js",
  "scripts": {
    "start": "npx nodemon server.js",
    "webpack:watch": "npx webpack --watch",
    "webpack:build": "npx webpack -p",
    "css:watch": "node-sass -rw client -o dist",
    "css:build": "node-sass client -o dist",
    "test": "jest ./src"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borispov/Baristagram.git"
  },
  "author": "Boris Povolotsky <boristofu@gmail.com>",
  "license": "ISC"
}
